{
  "name": "tells-service",
  "version": "0.41.0",
  "description": "Tells application backend",
  "private": true,
  "scripts": {
    "start:debug": "node -r dotenv/config --inspect=0.0.0.0:9226 ./dist/",
    "start:dev": "npm run prebuild && npm run copy:views && tsc-watch --outDir ./dist --onSuccess \"npm run start:debug\" --onFailure \"'echo Compilation Failed'\"",
    "start": "nodemon src/index.ts",
    "build": "rimraf ./dist && tsc && npm run copy:views",
    "prebuild": "rimraf dist",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "test": "jest",
    "prettier": "npx pretty-quick --staged --config ./.prettierrc",
    "get-version": "node -p \"require('./package.json').version\"",
    "migrate:up": "npm run build && npx sequelize-cli db:migrate",
    "migrate:down": "npm run build && npx sequelize-cli db:migrate:undo",
    "create:db": "node_modules/.bin/sequelize db:create",
    "seed": "npm run build && npx sequelize-cli db:seed:all",
    "unseed": "npm run build && npx sequelize-cli db:seed:undo",
    "copy:views": "copyfiles -f src/views/*.* dist/views/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@freewilltokyo/freewill-be": "^1.1.91",
    "@freewilltokyo/logger": "^1.0.4",
    "@freewilltokyo/srv": "^1.0.0",
    "adm-zip": "^0.5.9",
    "autobind-decorator": "^2.4.0",
    "aws-sdk": "^2.642.0",
    "content-type": "^1.0.4",
    "convict": "^5.2.0",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "dompurify": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^16.2.0",
    "helmet": "^3.21.2",
    "joi": "^14.3.1",
    "jsdom": "^16.2.2",
    "json2csv": "^5.0.6",
    "md5": "^2.2.1",
    "moment-timezone": "^0.5.34",
    "mysql2": "^2.1.0",
    "probe-image-size": "^5.0.0",
    "pug": "^2.0.4",
    "puppeteer": "^5.5.0",
    "request": "^2.88.2",
    "sequelize": "^5.21.5",
    "sequelize-cli": "^5.5.1",
    "stripe": "^8.39.0",
    "striptags": "^3.1.1",
    "uuid": "^7.0.2"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/content-type": "^1.1.3",
    "@types/convict": "^4.2.1",
    "@types/cors": "^2.8.6",
    "@types/dompurify": "^2.0.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.9.1",
    "@types/joi": "^14.3.4",
    "@types/jsdom": "^16.2.1",
    "@types/json2csv": "^5.0.3",
    "@types/md5": "^2.1.33",
    "@types/node": "^13.7.7",
    "@types/node-fetch": "^2.5.4",
    "@types/pug": "^2.0.6",
    "@types/puppeteer": "^5.4.6",
    "@types/request": "^2.48.4",
    "@types/sequelize": "^4.28.8",
    "@types/sharp": "^0.27.1",
    "@types/stripe": "^7.13.22",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/eslint-plugin-tslint": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^3.0.5",
    "jest": "^25.1.0",
    "lint-staged": "^9.4.0",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.0",
    "supertest": "^6.1.3",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "tsc-watch": "^4.0.0",
    "tslint": "~5.18.0",
    "tslint-config-security": "^1.16.0",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix",
      "pre-push": "npm test"
    }
  }
}
